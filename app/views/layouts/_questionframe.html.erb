
<%= link_to question_path( object ) do %>
<div class="">
  <div class="col-xs-10 col-xs-offset-1 question">
    <div class="col-xs-1">
       <h3><span class="glyphicon glyphicon-arrow-up glyph" aria-hidden="true"></span></h3>
        <h3><span class="glyphicon glyphicon-arrow-down glyph" aria-hidden="true"></span></h3>
    </div>
    <div class="col-xs-1">
      <label for="" class="label label-info">Votes</label>
      <h1> <%= object.votes %> </h1>
    </div>
    <div class="col-xs-1">
      <label for="" class="label label-info">Views</label>
      <h1> <%= object.views %> </h1>
    </div>
    <div class="col-xs-8">
      <h1><%= object.question %></h1>
    </div>
  </div>

  <div class="col-xs-4">
    <% getAllTags( object.tags ).each do |d| %>
      <label for="" class="label label-info">d</label>
    <%end%>
  </div>

  <div class="col-xs-1 col-xs-offset-5">
    <p> <%= object.owner %></p>
  </div>
</div>
<%end%>
